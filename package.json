{
  "name": "wallet-service",
  "version": "1.0.0",
  "description": "Wallet Service with Node.js",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --runInBand --detectOpenHandles",
    "test:coverage": "jest --coverage --runInBand --detectOpenHandles",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prepare": "husky install",
    "sonar:start": "docker-compose -f docker-compose.sonar.yml up -d",
    "sonar:stop": "docker-compose -f docker-compose.sonar.yml down",
    "sonar:analyze": "sonar-scanner"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@logtail/node": "^0.5.6",
    "@logtail/winston": "^0.5.6",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-jwt": "^8.5.1",
    "express-rate-limit": "^8.1.0",
    "firebase-admin": "^13.5.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "redis": "^5.8.2",
    "sequelize": "^6.37.1",
    "swagger-ui-express": "^5.0.0",
    "umzug": "^3.8.2",
    "winston": "^3.11.0",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.4",
    "@prisma/client": "^6.15.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.2",
    "nodemon": "^3.0.3",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "sonarqube-scanner": "^3.3.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
